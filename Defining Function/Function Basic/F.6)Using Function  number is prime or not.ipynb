   "source": [
    "def prime(a):\n",
    "    for i in range(2,a):\n",
    "        if a%i==0:\n",
    "            return(\"Not a prime number\")\n",
    "    return(\"This is prime number\")\n",
    "a=int(input(\"Enter a:\"))\n",
    "prime(a)"
   ]
